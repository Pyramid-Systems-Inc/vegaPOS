{
  "name": "vegaPOS",
  "version": "0.0.9",
  "description": "A minimal desktop POS application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build"
  },
  "repository": "https://github.com/Accelerate-net/vegaPOS",
  "keywords": [
    "POS",
    "Restaurant"
  ],
  "author": "Accelerate",
  "license": "CC0-1.0",
  "build": {
    "appId": "yourappid",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
  "devDependencies": {
    "electron": "~37.2.6",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9"
  },
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "electron-settings": "^4.0.4",
    "jquery": "^3.7.1",
    "leveldown": "^6.1.1",
    "mkdirp": "^3.0.1"
  }
}
